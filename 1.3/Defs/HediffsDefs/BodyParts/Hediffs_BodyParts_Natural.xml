<Defs>

	<ThingDef ParentName="BodyPartNaturalBase">
		<defName>UT_OrganicArm</defName>
		<label>arm</label>
		<description>A biological human arm. Attached to the shoulder, it allows the body to carry and manipulate things.</description>
		<descriptionHyperlinks>
			<RecipeDef>UT_InstallOrganicArm</RecipeDef>
		</descriptionHyperlinks>
		<statBases>
			<MarketValue>200</MarketValue>
			<Mass>3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
		<defName>UT_InstallOrganicArm</defName>
		<label>install arm</label>
		<description>Install a biological arm.</description>
		<descriptionHyperlinks>
			<ThingDef>UT_OrganicArm</ThingDef>
		</descriptionHyperlinks>
		<jobString>Installing arm.</jobString>
		<deathOnFailedSurgeryChance>0.05</deathOnFailedSurgeryChance>
		<skillRequirements>
			<Medicine>6</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>UT_OrganicArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>UT_OrganicArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Arm</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>

