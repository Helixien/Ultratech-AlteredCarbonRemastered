<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<RecipeDef Name="UT_RecipeHacking" Abstract="True">
		<workSpeedStat>ResearchSpeed</workSpeedStat>
		<effectWorking>UT_Hacking</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
	</RecipeDef>

	<RecipeDef>
		<defName>UT_DecryptAncientStack</defName>
		<label>decrypt ancient stack</label>
		<description>PLACEHOLDER</description>
		<jobString>decrypting ancient stack.</jobString>
		<workerClass>AlteredCarbon.Recipe_DecryptAncientCorticalStack</workerClass>
		<workAmount>180</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>UT_AncientStack</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>

	<RecipeDef>
		<defName>UT_HackBiocodedThings</defName>
		<label>hack biocoded things</label>
		<description>PLACEHOLDER</description>
		<jobString>hacking biocoded things.</jobString>
		<workerClass>AlteredCarbon.Recipe_HackBiocodedThings</workerClass>
		<workAmount>180</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Apparel_PowerArmorHelmet</li>
						<li>Gun_TripleRocket</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Apparel_PowerArmorHelmet</li>
				<li>Gun_TripleRocket</li>
			</thingDefs>
		</fixedIngredientFilter>
		<skillRequirements>
			<Intellectual>10</Intellectual>
		</skillRequirements>
	</RecipeDef>

	<RecipeDef>
		<defName>UT_WipeFilledCorticalStack</defName>
		<label>wipe filled cortical stack</label>
		<description>PLACEHOLDER</description>
		<jobString>wiping filled cortical stack.</jobString>
		<workerClass>AlteredCarbon.Recipe_WipeFilledCorticalStack</workerClass>
		<workAmount>180</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>UT_FilledCorticalStack</li>
						<li>UT_EmptyCorticalStack</li>
					</thingDefs>
					<specialFiltersToAllow>
						<li>UT_AllowStacksColonist</li>
						<li>UT_AllowStacksStranger</li>
						<li>UT_AllowStacksHostile</li>
					</specialFiltersToAllow>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>UT_FilledCorticalStack</li>
			</thingDefs>
			<specialFiltersToAllow>
				<li>UT_AllowStacksColonist</li>
				<li>UT_AllowStacksStranger</li>
				<li>UT_AllowStacksHostile</li>
			</specialFiltersToAllow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>UT_FilledCorticalStack</li>
			</thingDefs>
			<specialFiltersToAllow>
				<li>UT_AllowStacksColonist</li>
				<li>UT_AllowStacksStranger</li>
				<li>UT_AllowStacksHostile</li>
			</specialFiltersToAllow>
		</defaultIngredientFilter>
		<skillRequirements>
			<Intellectual>8</Intellectual>
		</skillRequirements>
	</RecipeDef>

</Defs>